<!-- Testimonial section start -->
<!-- Testimonial section end -->
<!-- Counter area start -->
<section class="counter__area pt-140 pb-140">
    <div class="container">
        <div class="counter__content">
            <h2 class="sec-title cxu-words">Dijitaldeki varlığınızı <br> görünür ve <br> güçlü hale getiriyoruz</h2>
            <p class="cxu-lines">
                İzmir merkezli firmamız, alanında uzman ekibimiz ile satıcı ve müşteri arasındaki güçlü köprüyü sağlam stratejilerle kuruyoruz
            </p>
            <div class="counter__list">
                <div class="counter__item style-1 cxu-fade" data-ease="back" data-delay="0.5">
                    <div class="counter-active number">25+</div>
                    <p>Takım Arkadaşımız </p>
                </div>
                <div class="counter__item style-1 cxu-fade" data-ease="back" data-delay="0.75">
                    <div class="counter-active number">7k+</div>
                    <p>Bizimle Çalışan Global Markalar</p>
                </div>
                <div class="counter__item style-1 cxu-fade" data-ease="back" data-delay="1">
                    <div class="counter-active number">56+</div>
                    <p>Yaratıcı Projeler</p>
                </div>
            </div>
        </div>
    </div>
    <img src="~/imgs/shape/arrow-10.png" alt="Shape" class="shape infinite-move">
    <img src="~/imgs/counter/1.png" alt="Image" class="counter__image cxu-fade" data-ease="bounce"
         data-speed="0.9">
</section>
<!-- Counter area end -->
<!-- Team area start -->
<!-- Team area end -->
<!-- CTA section start -->
<section class="cta__area cta__area-2 pt-150 pb-150">
    <div class="container">
        <div class="cta__content">
            <h2 class="sec-title cxu-words">GELİN, BİRLİKTE İNANILMAZ PROJELERE İMZA ATALIM</h2>
            <a href="contact.html" class="cxu-btn-primary cxu-fade" data-ease="bounce">
                Bizimle İletişime Geçin <i class="fa-solid fa-arrow-right"></i>
            </a>
        </div>
    </div>

    <img src="~/imgs/shape/cta.png" alt="Shape" class="shape" data-speed="0.89">
    <img src="~/imgs/shape/cta-2.png" alt="Shape" class="shape-2" data-speed="0.92">
    <img src="~/imgs/shape/cta-3.png" alt="Shape" class="shape-3" data-speed="0.9">
    <img src="~/imgs/shape/cta-4.png" alt="Shape" class="shape-4">
</section>
<!-- CTA section end -->

@model WebApplication5.Models.MVVM.MainPageModel
@{
    ViewData["Title"] = "AboutUs";
    Layout = "~/Views/Shared/DefaultLayout.cshtml";
}


      </header>
<!-- Header section end -->
<!-- Mobile Menu start -->
<div class="mobile__menu">
    <button class="close__menu"><i class="fa-solid fa-xmark"></i></button>
</div>
<!-- Mobile Menu end -->
<!-- Breadcrumb section start -->
<section class="breadcrumb__area">
    <div class="container">
        <div class="breadcrumb__content">
            <p class="cxu-words">Ana Sayfa - <a href="about-us.html">Hakkımızda</a></p>
            <h1 class="breadcrumb__title cxu-chars">Hakkımızda</h1>
        </div>
    </div>
</section>
<!-- Breadcrumb section end -->
<!-- About area start -->
<section class="about-inner__area pt-140 pb-140">
    <div class="container">
        <div class="about-inner__wrapper">
            <div class="about-inner__left">
                <img src="~/imgs/about/3.png" alt="Image" class="cxu-fade" data-ease="back" data-speed="0.9">
            </div>
            <div class="about-inner__right">
                <h2 class="title cxu-words">Uzman ekibimizle dijital varlığınızı güçlü stratejilerle oluşturuyor ve varlığını koruyoruz</h2>
                <p class="cxu-lines">
                    Grafik tasarım, web tasarım, yazılım hizmeti ve e-ticaret danışmanlığı ihtiyaçlarınızda kaliteli, detaylı, yaratıcı fikirler ve kalıcı çözümler sunuyoruz.
                </p>
                <a href="tel:@ViewBag.telephone" class="cxu-btn-primary cxu-fade" data-ease="bounce">
                    Bizi Arayın <i class="fa-solid fa-phone-volume"></i>
                </a>
            </div>
        </div>
    </div>
</section>
<!-- About area end -->
<!-- Counter area start -->
<section class="counter__area-4 pt-140 pb-140">
    <div class="container">
        <div class="counter__content">
            <div class="counter__list-4">
                <div class="counter__item style-3 cxu-fade" data-ease="back">
                    <div class="counter-active number">25+</div>
                    <p>Çözüm Ortağımız </p>
                </div>
                <div class="counter__item style-3 cxu-fade" data-ease="back">
                    <div class="counter-active number">7k+</div>
                    <p>Bizimle Çalışan Global Markalar</p>
                </div>
                <div class="counter__item style-3 cxu-fade" data-ease="back">
                    <div class="counter-active number">56+</div>
                    <p>Yaratıcı Projeler</p>
                </div>

            </div>
        </div>
    </div>
</section>
<!-- Counter area end -->
<!-- Service section start -->
<section class="service__area-2 pt-140 pb-140" id="services">
    <div class="container">
        <div class="service__top-2">
            <h2 class="sec-title cxu-words">Hizmetlerimiz</h2>
        </div>

        <div class="cxu__service style-2">
            <div class="service-item cxu-fade" data-ease="back">
                <div class="icon">
                    <img src="~/imgs/service/i1.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/EcommerceDetail">E-Ticaret <br> Danışmanlığı </a></h3>
                <p>Alanında uzman ekibimizden e-ticaret danışmanlığı alın.</p>
                <div class="link">
                    <a href="/Home/EcommerceDetail">Daha Fazla Bilgi <i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
            <div class="service-item cxu-fade" data-ease="back" data-delay="0.6">
                <div class="icon">
                    <img src="~/imgs/service/i2.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/WebDesignDetail">Web <br> Tasarım</a></h3>
                <p>Alanında uzman ekibimizden web tasarım hizmeti alın.</p>
                <div class="link">
                    <a href="/Home/WebDesignDetail">Daha Fazla Bilgi <i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
            <div class="service-item cxu-fade" data-ease="back" data-delay="0.8">
                <div class="icon">
                    <img src="~/imgs/service/i3.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/SoftwareDevelopmentDetail">Yazılım <br> Geliştirme</a></h3>
                <p>Alanında uzman ekibimizden yazılım geliştirme hizmeti alın.</p>
                <div class="link">
                    <a href="/Home/SoftwareDevelopmentDetail">Daha Fazla Bilgi <i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
            <div class="service-item cxu-fade" data-ease="back" data-delay="1">
                <div class="icon">
                    <img src="~/imgs/service/i4.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/GraphicDesignDetail">Grafik <br> Tasarım</a></h3>
                <p>Alanında uzman ekibimizden grafik tasarım hizmeti alın.</p>
                <div class="link">
                    <a href="/Home/GraphicDesignDetail">Daha Fazla Bilgi<i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
            <div class="service-item cxu-fade" data-ease="back">
                <div class="icon">
                    <img src="~/imgs/service/i5.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/SocialMediaDetail">Sosyal Medya <br> Yönetimi</a></h3>
                <p>Alanında uzman ekibimizden sosyal medya yönetimi danışmanlığı alın.</p>
                <div class="link">
                    <a href="/Home/SocialMediaDetail">Daha Fazla Bilgi<i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
            <div class="service-item cxu-fade" data-ease="back">
                <div class="icon">
                    <img src="~/imgs/service/i6.png" alt="Icon">
                </div>
                <h3 class="title"><a href="/Home/SEODetail">SEO  <br> Yönetimi</a></h3>
                <p>Alanında uzman ekibimizden SEO yönetimi danışmanlığı alın.</p>
                <div class="link">
                    <a href="/Home/SEODetail">Daha Fazla Bilgi <i class="fa-solid fa-arrow-right"></i></a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Service section end -->
